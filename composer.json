{
    "name": "checkybot/laravel-monitoring",
    "description": "Laravel package for defining and syncing monitoring checks to Checkybot",
    "keywords": ["laravel", "monitoring", "uptime", "api-monitoring", "ssl"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "MarinSolutions\\CheckybotLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarinSolutions\\CheckybotLaravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarinSolutions\\CheckybotLaravel\\CheckybotLaravelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
